import Model.PlaceModel;
global java.util.List resultList;
global org.kie.api.runtime.StatelessKieSession ks;
global Controller.RuleController rc;

rule "String"
    when
        $s:String($s == "John")
    then
        resultList.add(new PlaceModel("0", "home"));
end

rule "String2"
    when
        $s:String($s == "Fish")
    then
        resultList.add(new PlaceModel("1", "Ta le"));
end