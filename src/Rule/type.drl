package Rule;
global java.util.List resultList;
global java.util.List db;
global org.kie.api.runtime.StatelessKieSession ks;
global Controller.RuleController rc;


rule "String1"
    when
        $s:String($s == "Fish")
    then
        rc.type(1);

end
rule "String2"
    when
        $s:String($s == "Farm")
    then
        rc.type(2);

end

rule "String3"
    when
        $s:String($s == "Vegetable")
    then
        rc.type(2);

end